{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false, // Disable Windows authentication
    "anonymousAuthentication": true, // Enable anonymous authentication
    "iisExpress": {
      "applicationUrl": "http://localhost:5070", // URL for the IIS Express application
      "sslPort": 44372 // Port for SSL connections
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true, // Automatically open the browser
      "launchUrl": "swagger", // URL to launch on start (Swagger UI)
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development" // Set environment to Development
      }
    },

    "ExchangeRatesApi": {
      "commandName": "Project",
      "dotnetRunMessages": true, // Show .NET run messages in console
      "launchBrowser": true, // Automatically open the browser
      "launchUrl": "swagger", // URL to launch on start (Swagger UI)
      "applicationUrl": "https://localhost:7261;http://localhost:5070", // URLs for the application
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development" // Set environment to Development
      }
    }
  }
}
